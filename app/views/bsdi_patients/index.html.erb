<p id="notice"><%= notice %></p>

<h1>Listing Bsdi Patients</h1>

<table>
  <thead>
    <tr>
      <th>Bsdi</th>
      <th>Bsdi psmid</th>
      <th>Bsdi fname</th>
      <th>Bsdi lname</th>
      <th>Bsdi gender</th>
      <th>Bsdi birthdate</th>
      <th>Bsdi previous</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bsdi_patients.each do |bsdi_patient| %>
      <tr>
        <td><%= bsdi_patient.bsdi_id %></td>
        <td><%= bsdi_patient.bsdi_psmid %></td>
        <td><%= bsdi_patient.bsdi_fname %></td>
        <td><%= bsdi_patient.bsdi_lname %></td>
        <td><%= bsdi_patient.bsdi_gender %></td>
        <td><%= bsdi_patient.bsdi_birthdate %></td>
        <td><%= bsdi_patient.bsdi_previous_id %></td>
        <td><%= link_to 'Show', bsdi_patient %></td>
        <td><%= link_to 'Edit', edit_bsdi_patient_path(bsdi_patient) %></td>
        <td><%= link_to 'Destroy', bsdi_patient, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bsdi patient', new_bsdi_patient_path %>
